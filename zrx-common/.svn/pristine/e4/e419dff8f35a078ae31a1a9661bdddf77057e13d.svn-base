package com.zrx.hr.common.constants;

import lombok.Getter;

/**
 * Description: 售卖类型<br/>
 *
 * @author wangxiaoming
 * @date 2016年11月5日 下午2:31:58
 *
 */
public enum SaleType {

    SELF(1, "自营"),

    PLATFORM(2, "平台"),

    DISTRIBUTION(3, "分销");

    @Getter
    private int value;

    @Getter
    private String name;

    private SaleType(int value, String name) {
        this.value = value;
        this.name = name;
    }
}
